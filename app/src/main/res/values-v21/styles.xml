<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorPrimary">@color/theme_primary</item>
        <item name="android:colorPrimaryDark">@color/theme_primary_dark</item>
        <item name="android:colorAccent">@color/theme_accent_1</item>
        <item name="android:windowBackground">@color/windowBackgroundLight</item>
    </style>
    <style name="MyCustomToolBarTheme" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:textColorSecondary">#FFFFFF</item>
        <item name="android:windowNoTitle">false</item>
    </style>
    <style name="OverflowThem" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:textColorPrimary">#000000</item>
    </style>
    <style name="CardItem" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:textColorPrimary">#000000</item>
        <item name="android:textColorSecondary">#000000</item>
    </style>
    <style name="ToolbarTitle" parent="android:Theme.Material.Light">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
</resources>
